{
  "name": "git-server-mock",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "mock:dev": "nodemon app.ts",
    "build": "tsc",
    "generate-types": "npx openapi-typescript definitions/bitbucket-8.10.swagger.v3.json --output definitions/generated/bitbucket.ts",
    "lint": "npm run prettier:check",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "postinstall": "npm run generate-types"
  },
  "dependencies": {
    "@faker-js/faker": "8.0.0-beta.1",
    "dayjs": "1.11.7",
    "express": "4.18.1",
    "seedrandom": "3.0.5",
    "swagger-ui-express": "4.4.0"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "18.13.0",
    "@types/seedrandom": "3.0.2",
    "@types/swagger-ui-express": "4.1.3",
    "nodemon": "2.0.16",
    "openapi-typescript": "6.1.0",
    "prettier": "2.7.1",
    "simple-git": "3.16.0",
    "ts-node": "10.8.0"
  }
}
