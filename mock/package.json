{
  "name": "git-server-mock",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "mock:dev": "nodemon app.ts",
    "build": "npm run generate-types && tsc",
    "generate-types": "npx openapi-typescript definitions/bitbucket-8.10.swagger.v3.json --output definitions/generated/bitbucket.ts",
    "lint": "npm run prettier:check",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "postinstall": "npm run generate-types"
  },
  "dependencies": {
    "@faker-js/faker": "8.1.0",
    "dayjs": "1.11.10",
    "express": "4.18.2",
    "seedrandom": "3.0.5",
    "swagger-ui-express": "4.6.3"
  },
  "devDependencies": {
    "@types/express": "4.17.18",
    "@types/node": "18.18.1",
    "@types/seedrandom": "3.0.6",
    "@types/swagger-ui-express": "4.1.4",
    "nodemon": "2.0.22",
    "openapi-typescript": "6.7.0",
    "prettier": "2.8.8",
    "simple-git": "3.20.0",
    "ts-node": "10.9.1"
  }
}
